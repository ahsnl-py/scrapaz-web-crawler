{
    "name": "Apartment Listings",
    "baseSelector": ".list > div",
    "fields": [
        {
            "name": "name",
            "selector": "h2 > a",
            "type": "text"
        },
        {
            "name": "location",
            "selector": "h3",
            "type": "text"
        },
        {
            "name": "link",
            "selector": ".photos > a:first-child",
            "type": "attribute",
            "attribute": "href",
            "transform": "prepend_base_url"
        },
        {
            "name": "price",
            "selector": "strong",
            "type": "text",
            "transform": "strip"
        }
    ]
}