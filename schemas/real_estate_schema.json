{
  "name": "Apartment Listings",
  "baseSelector": ".list > div",
  "fields": [
      {
          "name": "name",
          "selector": "h3",
          "type": "text"
      },
      {
          "name": "location",
          "selector": "h2",
          "type": "text"
      },
      {
          "name": "link",
